                          <!--index.wxml-->

<swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" circular="{{circular}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item.url}}" class="swiper-image" />
    </swiper-item>
  </block>
</swiper>

<view class='menu_fa'>
  <view class='menus' wx:for="{{menus}}" wx:for-item="menu">
      <view class='menu'>
        <navigator url='{{menu.luj}}'>
        <image src='{{menu.url}}'></image>
        <text style='display:block'>{{menu.name}}</text>
        </navigator>
      </view>
  </view>
</view>





<view class='shop'>在售车型</view>
<swiper class="lun" style='background-color:#000;position:relative;' indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" circular="{{circular}}" duration="{{duration}}">
  <block wx:for="{{imgUrls1}}">
    <swiper-item >
    <view class='t2'>
      <view>{{item.name}}</view>
      <view>{{item.name1}}</view>
      <view>{{item.name2}}</view>
    </view>
    <view class='top' style='justify-content:space-between '>
      <view class='t1'>
        <image src="{{item.url}}" style='width:100%;height:100%;' />
      </view>
      <view class='t1'>
        <image src="{{item.url1}}" style='width:100%;height:100%;' />
      </view>
      <view class='t1'>
        <image src="{{item.url2}}" style='width:100%;height:100%;' />
      </view>
    </view>
    </swiper-item>
  </block>
</swiper>


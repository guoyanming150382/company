<view class="row-wrap">
  <view class="label" style='width:100%;font-size:50rpx;line-height:100rpx;margin:100rpx 0;text-align:center;font-weight:600;'>请选择预约项目</view>
  <picker style='width:100%;line-height:100rpx;text-align:center;' bindchange="bindCasPickerChange" value="{{casIndex1}}" range="{{casArray}}">
    <view>
      <text style='display:inline-block;line-height:100rpx;border-radius:50rpx;padding:0 30rpx;border:1px solid #ccc'>{{casArray[casIndex]}}</text>
    </view>
  </picker>
</view>
<view class="section {{reply?'on':'off'}}">
  <input name="other" placeholder="请输入所预约项目" type="text" />
</view>

